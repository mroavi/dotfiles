# This tmux statusbar config was created by tmuxline.vim
# on wo, 10 jun 2020

set -g status-justify "left"
set -g status "on"
set -g status-left-style "none"
set -g message-command-style "fg=#C0C5CE,bg=#4F5B66"
set -g status-right-style "none"
set -g pane-active-border-style "fg=#6699CC"
set -g status-position "top"
set -g status-style "none,bg=#343D46"
set -g message-style "fg=#C0C5CE,bg=#4F5B66"
set -g pane-border-style "fg=#4F5B66"
set -g status-right-length "100"
set -g status-left-length "100"
setw -g window-status-activity-style "none,fg=#6699CC,bg=#343D46"
setw -g window-status-separator ""
setw -g window-status-style "none,fg=#C0C5CE,bg=#343D46"
set -g status-left "#[fg=#343D46,bg=#6699CC,bold] #S #[fg=#6699CC,bg=#343D46,nobold,nounderscore,noitalics]î‚°"
set -g status-right "#[fg=#4F5B66,bg=#343D46,nobold,nounderscore,noitalics]î‚²#[fg=#C0C5CE,bg=#4F5B66] ğŸ•%R î‚³ ğŸ“…%D #[fg=#6699CC,bg=#4F5B66,nobold,nounderscore,noitalics]î‚²#[fg=#343D46,bg=#6699CC] ğŸ‘¤#(whoami) "
setw -g window-status-format "#[fg=#343D46,bg=#343D46,nobold,nounderscore,noitalics]î‚°#[default] #I #W #[fg=#343D46,bg=#343D46,nobold,nounderscore,noitalics]î‚°"
setw -g window-status-current-format "#[fg=#343D46,bg=#4F5B66,nobold,nounderscore,noitalics]î‚°#[fg=#C0C5CE,bg=#4F5B66] #I î‚± #W #[fg=#4F5B66,bg=#343D46,nobold,nounderscore,noitalics]î‚°"
